about: this file generates the algoritmeregister-metadata-standard.json file from the yaml
---
[
    {% for item in site.data.algoritmeregister-metadata-standaard %}
        {
            {% for it in item %}
                "{{ it[0] }}": "{{ it[1] }}"{% if forloop.last == false %},{% endif %}
            {% endfor %}
        }{% if forloop.last == false %},{% endif %}
    {% endfor %}
]