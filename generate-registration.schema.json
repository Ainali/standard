---
about: this file generates the registration.schema.json file from the yaml
---
{
    "$id": "{{ site.url }}/schemas/registration.schema.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Registration",
    "description": "A registration of the use of an algorithmic application by a government organization",
    "type": "object",
    "properties": {
        {% for item in site.data.algorithm-register-metadata-standard-suggestions %}
            "{{ item.property }}": {
                {% for it in item %}
                    "{{ it[0] }}": "{{ it[1] }}"{% if forloop.last == false %},{% endif %}
                {% endfor %}
            }{% if forloop.last == false %},{% endif %}
        {% endfor %}
    }
}